{
    "name": "nathanlon/xero-bundle",
    "description": "Symfony bundle wrapping the XeroPHP library",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "keywords": ["xero", "xero-php"],
    "require": {
        "php": ">=5.5.9",
        "nathanlon/xero-php": "dev-master",
        "symfony/config": "^2.7|^3.0|^4.2",
        "symfony/debug": "^2.7|^3.0|^4.2",
        "symfony/dependency-injection": "^2.7|^3.0|^4.2",
        "symfony/framework-bundle": "^2.7|^3.0|^4.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Four Labs",
            "email": "jonny@fourlabs.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "FL\\XeroBundle\\": ""
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:nathanlon/xero-php.git"
        }
    ]
}

